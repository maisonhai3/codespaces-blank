[SERVICE]
    Flush         5
    Log_Level     info
    Daemon        off
    Parsers_File  parsers.conf

[INPUT]
    Name              tail
    Path              /shared-logs/aiscout.log
    Tag               aiscout.logs
    Parser            json
    Refresh_Interval  1
    Read_from_Head    true

[OUTPUT]
    Name  es
    Match *
    Host  elasticsearch
    Port  9200
    Index aiscout-logs
    Retry_Limit 5
    Suppress_Type_Name On
